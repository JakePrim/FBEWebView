<?xml version="1.0" encoding="UTF-8" ?>
<webview-mapper namespace="channel">
    <!-- webview的配置 -->
    <webview>
        <!-- webview 自定义容器类,必须实现IWebView接口，如果不设置会使用默认的primweb实现 -->
        <!--        <property name="container" value="" />-->
        <!-- webSetting 类 -->
        <property name="webSetting" value="" />
        <!-- webChromeClient类 -->
        <property name="webChromeClient" value="" />
        <!-- WebViewClient类 -->
        <property name="webViewClient" value="" />
        <!-- 主要为primweb进行配置 -->
        <property name="webViewType" value="Android" />
        <!-- 监听js方法是否存在类 -->
        <property name="listenerCheckJsFunction" value="" />
        <!-- UI配置 -->
        <!-- 错误页面： value 是布局文件的名称：R.layout.error 直接填写error即可; clickId:错误页面点击某个view触发的点击事件，填写规则：R.id.text 直接写text即可 -->
        <property name="errorLayout" value="error" clickId="error_text" />
        <!-- 加载页面 -->
        <property name="loadLayout" value="load" />
        <!-- 是否启用顶部指示器 -->
        <property name="enableTopIndicator" value="true" />
        <!-- 顶部指示器的颜色和高度设置 -->
        <property name="topIndicator" colorInt="teal_200" height="10" />
    </webview>
</webview-mapper>